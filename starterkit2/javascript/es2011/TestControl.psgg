sheet=template-statefunc

###VARIOUS-CONTENTS-BEGIN###
<<<?state/^C_/
eof>>>
/*
    [[state]]
    [[state-cmt]]
*/
<<<?state/^E_/
[[embed]]
eof>>>

<<<?state-typ/^start$/
TestControl.prototype.[[state]] = function(first) {
    this.goto(this.[[nextstate]]);
    this.setnowait();
};
eof>>>

<<<?state-typ/^end$/
TestControl.prototype.[[state]] = function(first) {
};
eof>>>

<<<?state-typ/^gosub$/
TestControl.prototype.[[state]] = function(first) {
    this.gosubstate(this.[[gosubstate]],this.[[nextstate]]);
    this.setnowait();
};
eof>>>

<<<?state-typ/^substart$/
TestControl.prototype.[[state]] = function(first) {
    this.goto(this.[[nextstate]]);
    this.setnowait();
};
eof>>>

<<<?state-typ/^subreturn$/
TestControl.prototype.[[state]] = function(first) {
    this.returnstate();
    this.setnowait();
};
eof>>>

<<<?state-typ/^loop$/
TestControl.prototype.[[state]] = function(first) {
    [[loop_init]]
    this.goto(this.[[state]]_LoopCheckAndGosub____);
    this.setnowait();
};
TestControl.prototype.[[state]]_LoopCheckAndGosub____ = function(first) {
    if ([[loop_cond]]) this.gosubstate(this.[[gosubstate]],this.[[state]]_LoopNext____);
    else               this.goto(this.[[nextstate]]);
    this.setnowait();
};
TestControl.prototype.[[state]]_LoopNext____ = function(first) {
    [[loop_next]]
    this.goto(this.[[state]]_LoopCheckAndGosub____);
    this.setnowait();
};
eof>>>

TestControl.prototype.[[state]] = function(first) {

    [[vars]]

    <<<?init
    if (first)
    {
        [[init]]
    }
    >>>

    [[update]]

    <<<?wait
    if ([[wait]]) { return; }
    >>>

    [[post_wait]]

    [[branch]]

    <<<?nextstate
    if (!this.hasnext()) {
        this.goto(this.[[nextstate]]);
    }
    >>>

    <<<?nowait
    if (this.hasnext()) {
        this.setnowait();
    }
    >>>
};
###VARIOUS-CONTENTS-END###


